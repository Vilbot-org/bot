{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": [
		"airbnb-base",
		"plugin:prettier/recommended",
		"eslint:recommended"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "error",
		"no-unused-vars": "warn",
		"no-console": "warn",
		"spaced-comment": "error"
	},
	"settings": {
		"import/resolver": {
			"babel-module": {
				"root": ["./"],
				"alias": {
					"@": "./src"
				}
			}
		}
	}
}
